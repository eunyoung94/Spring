package test;

public class UseStudent {

	public static void main(String[] args) {
		Student st = new Student();
		Bell bell= new Bell();
		
		bell.StartBell();
		st.study1();
		bell.EndBell();
		
		bell.StartBell();
		st.study2();
		bell.EndBell();
	
		bell.StartBell();
		st.study3();
		bell.EndBell();
		
		bell.StartBell();
		st.study4();
		bell.EndBell();
		
	}
}
